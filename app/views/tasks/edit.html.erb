<h1> Edit Tasks</h1>

<%= form_with model:@task do |f| %>
    <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
    </div>

    <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
    </div>

    <div>
    <%= f.submit "update task" %>
    </div>
    <% end %>
    